{
  "name": "@watchmen/web-helpr",
  "version": "2.5.0",
  "main": "dist/index.js",
  "author": "tony <anthony.kerz@gmail.com>",
  "license": "MIT",
  "scripts": {
    "ava": "cross-env DEBUG=test:*,app:* ava",
    "pretest": "xo",
    "test": "npm run ava",
    "prebuild": "npm run test",
    "build": "rm -rf ./dist && babel ./src -d ./dist"
  },
  "dependencies": {
    "@watchmen/helpr": "2.3.8",
    "axios": "0.19.0",
    "debug": "4.1.1",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.4.5",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/register": "7.4.4",
    "ava": "1.4.1",
    "cross-env": "5.2.0",
    "jsonwebtoken": "8.5.1",
    "jws": "3.2.2",
    "xo": "0.24.0"
  },
  "ava": {
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ]
  },
  "xo": {
    "prettier": true,
    "rules": {
      "radix": 0,
      "no-unused-expressions": [
        2,
        {
          "allowShortCircuit": true,
          "allowTernary": true
        }
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
